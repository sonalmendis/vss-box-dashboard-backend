      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "markdown"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "sources": {
      "displayName": "sources",
      "type": "component",
      "repeatable": true,
      "component": "sources.sources"
    }
